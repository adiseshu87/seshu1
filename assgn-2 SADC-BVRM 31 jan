#include <stdio.h>

int main()
{
    
    int i,n,a[100];
    
    printf("enter number of elements ");
    scanf("%d",&n);
    printf("enter %d elements ",n);
    for(i=0;i<n;i++)
    scanf("%d",&a[i]);
    int c=0;
    for(i=0;i<n;i++)
    {
        if(a[i]!=0)
        {
            a[c]=a[i];
            c++;
        }
    }
    for(i=0;i<n;i++){
    a[c]=0;
    c++;
}
for(i=0;i<n;i++)
{
    printf("%d",a[i]);
}

      
       

    return 0;
}
